{
  "name": "budget",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bun run ./src/lib/server/db/migrate.ts",
    "db:seed": "bun run ./src/lib/server/db/seeders",
    "db:reseed": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/seeders",
    "db:factory": "bun run ./src/lib/server/db/factories",
    "db:refactory": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/factories",
    "db:drop": "bunx drizzle-kit drop && rm -f ./drizzle/db/sqlite.db",
    "db:restart": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed",
    "db:restart:test": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed && bun run db:factory"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@lucide/svelte": "^0.526.0",
    "@playwright/test": "^1.55.0",
    "@sinclair/typebox": "^0.34.41",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.37.0",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
    "@tailwindcss/vite": "^4.1.12",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.21",
    "@typeschema/class-validator": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "arktype": "^2.1.20",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.10.0",
    "bun-types": "^1.2.21",
    "class-validator": "^0.14.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-svelte": "^3.11.0",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.6",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.3.1",
    "svelte-render-scan": "^1.1.0",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@decs/typeschema": "^0.12.2",
    "@internationalized/date": "^3.9.0",
    "@libsql/client": "^0.15.14",
    "@orpc/client": "^1.8.5",
    "@orpc/server": "^1.8.5",
    "@orpc/tanstack-query": "^1.8.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/svelte-query": "^5.85.5",
    "@tanstack/svelte-query-devtools": "^5.85.5",
    "@tanstack/table-core": "^8.21.3",
    "@thisux/sveltednd": "^0.0.20",
    "bits-ui": "^2.9.4",
    "caniuse-lite": "^1.0.30001737",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.5",
    "drizzle-valibot": "^0.4.2",
    "drizzle-zod": "^0.8.3",
    "formsnap": "^2.0.1",
    "fuse.js": "^7.1.0",
    "paneforge": "^1.0.2",
    "runed": "^0.31.1",
    "shadcn-svelte": "^1.0.7",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "vaul-svelte": "^1.0.0-next.7",
    "zod": "4.0.10"
  }
}
