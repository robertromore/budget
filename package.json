{
  "name": "budget",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bun run ./src/lib/server/db/migrate.ts",
    "db:seed": "bun run ./src/lib/server/db/seeders",
    "db:reseed": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/seeders",
    "db:factory": "bun run ./src/lib/server/db/factories",
    "db:refactory": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/factories",
    "db:drop": "bunx drizzle-kit drop && rm -f ./drizzle/db/sqlite.db",
    "db:restart": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed",
    "db:restart:test": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed && bun run db:factory"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.1.0",
    "@iconify-json/carbon": "^1.2.4",
    "@iconify-json/lucide": "^1.2.13",
    "@iconify-json/radix-icons": "^1.2.1",
    "@iconify/tailwind": "^1.1.3",
    "@playwright/test": "^1.48.2",
    "@sveltejs/adapter-auto": "^3.2.0",
    "@sveltejs/kit": "^2.7.4",
    "@sveltejs/vite-plugin-svelte-inspector": "^3.0.1",
    "@types/better-sqlite3": "^7.6.11",
    "@types/bun": "^1.1.14",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^11.5.0",
    "bun-types": "^1.2.2",
    "drizzle-kit": "^0.30.4",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-svelte": "^2.46.0",
    "lucide-svelte": "^0.471.0",
    "postcss": "^8.4.47",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "svelte": "^5.19.9",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.0.5",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@internationalized/date": "^3.7.0",
    "@libsql/client": "^0.14.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/table-core": "^8.20.5",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "bits-ui": "^1.0.0-next.71",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.39.2",
    "drizzle-zod": "^0.7",
    "formsnap": "^2.0.0-next.1",
    "runed": "^0.23.2",
    "shadcn-svelte": "^1.0.0-next",
    "svelte-radix": "^2.0.1",
    "sveltekit-superforms": "^2.23.1",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-animate": "^1.0.7",
    "trpc-sveltekit": "^3.6.2",
    "trpc-transformer": "^3.2.2",
    "zod": "^3.24.1"
  }
}
