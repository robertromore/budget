{
  "name": "budget",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "db:generate": "bunx drizzle-kit generate:sqlite",
    "db:migrate": "bun run ./src/lib/server/db/migrate.ts",
    "db:seed": "bun run ./src/lib/server/db/seeders",
    "db:reseed": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/seeders",
    "db:factory": "bun run ./src/lib/server/db/factories",
    "db:refactory": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/factories",
    "db:drop": "bunx drizzle-kit drop && rm -f ./drizzle/db/sqlite.db",
    "db:restart": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@iconify-json/carbon": "^1.1.31",
    "@iconify-json/lucide": "^1.1.177",
    "@iconify-json/radix-icons": "^1.1.14",
    "@iconify/tailwind": "^0.1.4",
    "@playwright/test": "^1.42.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/better-sqlite3": "^7.6.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "^9.4.3",
    "bun-types": "^1.0.35",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "svelte": "^5.0.0-next.96",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.6.8",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.6",
    "vitest": "^1.4.0"
  },
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.2",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@tanstack/table-core": "^8.14.0",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "bits-ui": "^0.21.3",
    "clsx": "^2.1.0",
    "cmdk-sv": "^0.0.16",
    "drizzle-orm": "^0.30.4",
    "drizzle-zod": "^0.5.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "formsnap": "^0.5.1",
    "shadcn-svelte": "^0.9.3",
    "svelte-breadcrumbs": "^1.2.7",
    "svelte-headless-table": "^0.18.2",
    "svelte-radix": "^1.1.0",
    "sveltekit-superforms": "^2.11.0",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "trpc-sveltekit": "^3.6.1",
    "trpc-transformer": "^3.2.1",
    "zod": "^3.22.4"
  }
}
