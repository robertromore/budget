{
  "name": "budget",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "db:generate": "bunx drizzle-kit generate:sqlite",
    "db:migrate": "bun run ./src/lib/server/db/migrate.ts",
    "db:seed": "bun run ./src/lib/server/db/seeders",
    "db:reseed": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/seeders",
    "db:factory": "bun run ./src/lib/server/db/factory",
    "db:refactory": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/factory",
    "db:drop": "bunx drizzle-kit drop && rm -f ./drizzle/db/sqlite.db",
    "db:restart": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@iconify/svelte": "^3.1.6",
    "@paralleldrive/cuid2": "^2.2.2",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@trpc/server": "^10.45.0",
    "@types/better-sqlite3": "^7.6.8",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^9.2.2",
    "bun-types": "^1.0.20",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^5.0.0-next.1",
    "svelte-adapter-bun": "^0.5.1",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "trpc-sveltekit": "^3.5.22",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.0.0",
    "zod": "^3.22.4"
  },
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.1",
    "@sindresorhus/slugify": "^2.2.1",
    "bits-ui": "^0.15.1",
    "clsx": "^2.0.0",
    "cmdk-sv": "^0.0.13",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "eslint-plugin-drizzle": "^0.2.3",
    "formsnap": "^0.4.2",
    "radix-icons-svelte": "^1.2.1",
    "svelte-breadcrumbs": "^1.2.0",
    "svelte-headless-table": "^0.18.1",
    "sveltekit-superforms": "^1.13.2",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.1.19"
  }
}
