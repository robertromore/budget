{
  "name": "budget",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bun run ./src/lib/server/db/migrate.ts",
    "db:seed": "bun run ./src/lib/server/db/seeders",
    "db:reseed": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/seeders",
    "db:factory": "bun run ./src/lib/server/db/factories",
    "db:refactory": "bun run ./src/lib/server/db/delete-all && bun run ./src/lib/server/db/factories",
    "db:drop": "bunx drizzle-kit drop && rm -f ./drizzle/db/sqlite.db",
    "db:restart": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed",
    "db:restart:test": "bun run db:drop && bun run db:generate && bun run db:migrate && bun run db:seed && bun run db:factory"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@iconify-json/carbon": "^1.2.8",
    "@iconify-json/lucide": "^1.2.37",
    "@iconify-json/radix-icons": "^1.2.2",
    "@iconify/tailwind": "^1.2.0",
    "@playwright/test": "^1.52.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.10",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.9.1",
    "bun-types": "^1.2.10",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-svelte": "^2.46.1",
    "lucide-svelte": "^0.475.0",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.27.3",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.1.6",
    "svelte-render-scan": "^1.1.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.2",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@internationalized/date": "^3.8.0",
    "@libsql/client": "^0.14.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/table-core": "^8.21.3",
    "@thisux/sveltednd": "^0.0.20",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "bits-ui": "^1.3.19",
    "caniuse-lite": "^1.0.30001714",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.1",
    "formsnap": "^2.0.1",
    "fuse.js": "^7.1.0",
    "runed": "^0.23.4",
    "shadcn-svelte": "^1.0.0-next.9",
    "svelte-radix": "^2.0.1",
    "sveltekit-superforms": "2.22.0",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7",
    "trpc-sveltekit": "^3.6.3",
    "trpc-transformer": "^3.2.2",
    "zod": "^3.24.3"
  }
}
