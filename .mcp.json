{
  "mcpServers": {
    "svelte-llm": {
      "type": "http",
      "url": "https://svelte-llm.stanislav.garden/mcp/mcp"
    },
    "browser-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["@browser-mcp/mcp-server"]
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    }
  }
}
